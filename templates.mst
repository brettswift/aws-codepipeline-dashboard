{{#pipelines}}
<div class="pipeline">
  <div class="pipelineName">{{name}}</div>
  <ul class="pipelineStages">
  {{#stages}}
      <li class="pipelineStage {{status}}">
        {{name}} 
        <ul class="actions">
        {{#actions}}
            <li class="action {{status}}">{{name}}</li>
        {{/actions}}
        </ul>
      </li>
  {{/stages}}
  </ul>
</div>
{{/pipelines}}


